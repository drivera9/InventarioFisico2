<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.mono.inventariofisico2.GuardarProducto"
    android:id="@+id/GUARDAR">

    <LinearLayout
        style="@style/proyectoWMS_METRO_Theme.Layout.LinearLogin">

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="330dp"
        android:id="@+id/gridView5"
        android:numColumns="2"
        android:gravity="left"/>
        <Button
            style="@style/proyectoWMS_METRO_Theme.Button.AceptarLogin"
            android:text="Guardar"
            android:id="@+id/button_guardar"
            android:onClick="guardar" />
        <Button
            style="@style/proyectoWMS_METRO_Theme.Button.CancelarLogin"
            android:text="Regresar"
            android:id="@+id/button_regresar"
            android:onClick="regresar" />
    </LinearLayout>
</RelativeLayout>
